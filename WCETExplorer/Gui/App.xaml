<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:SampleData="clr-namespace:Expression.Blend.SampleData.WCETLine" 
			 xmlns:SampleData1="clr-namespace:Expression.Blend.SampleData.AVGLine" 
			 xmlns:SampleData2="clr-namespace:Expression.Blend.SampleData.FunktionNames" 
			 mc:Ignorable="d" 
			 x:Class="Gui.App"
             StartupUri="WAlgorithmSettings.xaml">
    <Application.Resources>
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="Simple Styles.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
        
    		<SampleData2:FunktionNames x:Key="FunktionNames" d:IsDataSource="True"/>
        
    		<SampleData1:AVGLine x:Key="AVGLine" d:IsDataSource="True"/>
        
    		<SampleData:WCETLine x:Key="WCETLine" d:IsDataSource="True"/>
        
    	</ResourceDictionary>
        
    </Application.Resources>
</Application>
